package bai_01;

public class ToaDo {
	private char ten;
	private float x, y;
	
	public ToaDo() {
	}
	public char getTen() {
		return ten;
	}
	public void setTen(char ten) {
		this.ten = ten;
	}
	public float getX() {
		return x;
	}
	public void setX(float x) {
		this.x = x;
	}
	public float getY() {
		return y;
	}
	public void setY(float y) {
		this.y = y;
	}
	public ToaDo(char ten, float x, float y) {
		this.ten = ten;
		this.x = x;
		this.y = y;
	}
	public String getThongTin() {
		return ten + "(" + x + ", "+ y + ")";
	}
	
}
